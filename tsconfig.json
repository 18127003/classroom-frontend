{
    "compilerOptions": {
      "target": "ES5",
      "allowJs": true,
      "strict": false,
      "module": "ESNext",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "sourceMap": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@@/*": ["./*"]
      }
    },
    "include": ["src/**/*", "app.d.ts"]
}